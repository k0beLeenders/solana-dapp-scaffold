{
  "name": "react-scaffold-template",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/k0beLeenders/solana-dapp-scaffold.git"
  },
  "homepage": ".",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "program:build": "cd program && cargo build-bpf",
    "program:test": "cd program && cargo test-bpf",
    "localnet:up": "solana-test-validator --bpf-program BpfProgram1111111111111111111111111111111111 program/target/deploy/bpf_program_template.so --reset",
    "localnet:logs": "solana logs -u localhost",
    "predeploy": "git pull --ff-only && yarn && yarn build",
    "deploy": "gh-pages -d build",
    "deploy:ar": "arweave deploy-dir build --key-file ",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\""
  },
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@project-serum/anchor": "^0.24.2",
    "@project-serum/serum": "^0.13.65",
    "@reduxjs/toolkit": "^1.8.1",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/spl-token": "^0.2.0",
    "@solana/spl-token-registry": "^0.2.203",
    "@solana/spl-token-swap": "^0.1.3",
    "@solana/wallet-adapter-ant-design": "^0.11.4",
    "@solana/wallet-adapter-base": "^0.9.5",
    "@solana/wallet-adapter-react": "^0.15.4",
    "@solana/wallet-adapter-react-ui": "^0.9.6",
    "@solana/wallet-adapter-wallets": "^0.16.0",
    "@solana/web3.js": "^1.43.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-slick": "^0.23.4",
    "@types/redux-logger": "^3.0.9",
    "antd": "^4.21.3",
    "assert": "^2.0.0",
    "axios": "^0.27.2",
    "bn.js": "^5.2.0",
    "bs58": "^5.0.0",
    "craco-less": "^2.0.0",
    "eventemitter3": "^4.0.7",
    "export": "^0.1.337",
    "fs": "^0.0.1-security",
    "https-browserify": "^1.0.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "os": "^0.1.2",
    "path-browserify": "^1.0.1",
    "react": "^18.1.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.31.1",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.0.1",
    "redux-logger": "^3.0.6",
    "slick-carousel": "^1.8.1",
    "source-map-loader": "^3.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "arweave-deploy": "^1.9.1",
    "gh-pages": "^3.1.0",
    "prettier": "^2.6.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!@project-serum/sol-wallet-adapter)"
    ]
  }
}
